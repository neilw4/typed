{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google"],
    "rules": {
        "arrow-parens": ["warn", "as-needed"],
        "eqeqeq": "error",
        "max-len": ["warn", 100],
        "no-unused-vars": "warn",
        "no-warning-comments": "warn",
        "require-jsdoc": "warn",
        "semi": ["error", "never"]
    }
}
